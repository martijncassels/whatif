div(class='btn-group',role='group',aria-label='Basic Example')
	a(href='profiles/view/{{profile._id}}').btn.btn-sm.btn-primary
		i(aria-hidden="true",data-toggle="tooltip",data-placement="top",title="Back to profiles").fa.fa-arrow-left
